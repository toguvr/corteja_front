{
  "name": "@toolpad/utils",
  "version": "0.13.0",
  "description": "Shared utilities used by Toolpad packages.",
  "author": "MUI Toolpad team",
  "homepage": "https://github.com/mui/toolpad#readme",
  "license": "MIT",
  "main": "./index.js",
  "module": "./esm/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "mui-modern": {
        "types": "./modern/index.d.ts",
        "default": "./modern/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./*.d.ts",
        "default": "./*.js"
      },
      "import": {
        "types": "./esm/*.d.ts",
        "default": "./esm/*.js"
      },
      "mui-modern": {
        "types": "./modern/*.d.ts",
        "default": "./modern/*.js"
      }
    },
    "./hooks/*": {
      "require": {
        "types": "./hooks/*.d.ts",
        "default": "./hooks/*.js"
      },
      "import": {
        "types": "./esm/hooks/*.d.ts",
        "default": "./esm/hooks/*.js"
      },
      "mui-modern": {
        "types": "./modern/hooks/*.d.ts",
        "default": "./modern/hooks/*.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/toolpad.git",
    "directory": "packages/toolpad-studio-utils"
  },
  "bugs": {
    "url": "https://github.com/mui/toolpad/issues"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "invariant": "2.2.4",
    "prettier": "3.4.2",
    "react-is": "^19.0.0",
    "title": "4.0.1",
    "yaml": "2.5.1",
    "yaml-diff-patch": "2.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "private": false
}